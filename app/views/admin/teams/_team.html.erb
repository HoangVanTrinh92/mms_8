<table>
  <thead class="t-header">
    <tr>
      <th class="th1"><%= t "team.name" %></th>
      <th class="th2"><%= t "team.description" %></th>
      <th class="th3"><%= t "team.leader" %></th>
      <th colspan="3" class="th-colspan"></th>
    </tr>
  </thead>

  <tbody class="t-body">
      <tr>
        <td><%= link_to team.name, admin_team_path(team) %></td>
        <td><%= team.description %></td>
        <td><%= team.leader %></td>
        <td><%= link_to t("user.show"), admin_team_path(team) %></td>
        <td><%= link_to t("user.edit"), edit_admin_team_path(team) %></td>
        <td><%= link_to t("user.delete"), admin_team_path(team), method: :delete,
          data: {confirm: t("user.confirm")} %></td>
      </tr>
  </tbody>
</table>
