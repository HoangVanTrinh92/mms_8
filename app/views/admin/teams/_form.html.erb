<%= form_for [:admin, @team] do |f| %>
  <%= render "error" %>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :leader %><br>
    <%= f.text_field :leader %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
